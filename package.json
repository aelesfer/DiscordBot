{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "Bot para Discord usado en el canal RoleandoOnline",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "./node_modules/.bin/tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "node dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "./node_modules/.bin/tsc -w",
    "watch-node": "nodemon dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aelesfer/DiscordBot.git"
  },
  "keywords": [
    "discord",
    "bot",
    "node",
    "typescript"
  ],
  "author": "aelesfer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aelesfer/DiscordBot/issues"
  },
  "homepage": "https://github.com/aelesfer/DiscordBot#readme",
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "discord.js": "^11.3.0",
    "express": "^4.16.2",
    "googleapis": "^25.0.0",
    "moment": "^2.20.1",
    "node-cron": "^1.2.1",
    "striptags": "^3.1.1",
    "util": "^0.10.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/lodash": "^4.14.93",
    "@types/node": "^9.3.0",
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.11",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": "8.9.4",
    "npm": "5.6.0"
  }
}
